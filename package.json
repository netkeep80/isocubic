{
  "name": "isocubic",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue,js,jsx,json,css}\" \"*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,vue,js,jsx,json,css}\" \"*.{ts,js,json}\"",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "metamode:validate": "tsx scripts/metamode-preprocessor.ts --check",
    "metamode:validate:verbose": "tsx scripts/metamode-preprocessor.ts --check --verbose",
    "metamode:compile": "tsx scripts/metamode-preprocessor.ts --compile",
    "metamode:ai": "tsx scripts/metamode-preprocessor.ts --ai",
    "metamode:ai:analyze": "tsx scripts/metamode-ai-optimizer.ts --analyze",
    "metamode:parse": "tsx scripts/metamode-annotation-parser.ts",
    "metamode:validate:semantic": "tsx scripts/metamode-semantic-validator.ts",
    "metamode:migrate": "tsx scripts/metamode-migrate.ts",
    "metamode:migrate:apply": "tsx scripts/metamode-migrate.ts --apply",
    "metamode:db:compile": "tsx scripts/metamode-db-compiler.ts --stats",
    "metamode:db:graph": "tsx scripts/metamode-db-compiler.ts --graph",
    "metamode:generate-tests": "tsx scripts/metamode-test-generator.ts",
    "metamode:generate-tests:dry": "tsx scripts/metamode-test-generator.ts --dry-run",
    "check:all": "npm run lint && npm run typecheck && npm run format:check && npm run metamode:validate && npm run test:run"
  },
  "dependencies": {
    "@tresjs/cientos": "^5.2.5",
    "@tresjs/core": "^5.3.3",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "pinia": "^3.0.2",
    "three": "^0.182.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.10.1",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/compiler-dom": "^3.5.27",
    "@vue/server-renderer": "^3.5.27",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.8"
  }
}
